<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.gegy1000.slyther.client.db.GameStatistic" table="games">
		<id name="gamedate" column="gamedate" />
		<property name="duration" />
		<property name="kills" />
		<property name="length" />
		<property name="rank" />
		<property name="snakeCount" />
	</class>
</hibernate-mapping>
